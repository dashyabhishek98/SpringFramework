<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
<bean id="c" class="beans.Car">
<property name="carName" >
<list>
<value>Alto</value>
<value>WaganR</value>
<value>Audi</value>
<value>Duster</value>
</list>
</property>
<property name="engine" >
<list>
<ref bean="e1"/>
<ref bean="e2"/>
<ref bean="e3"/>
<ref bean="e4"/>
</list>
</property>
</bean>
</beans>